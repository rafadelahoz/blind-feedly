{
	"manifest_version": 2,
	
	"name": "Blind Feedly",
	"description": "Hides images in articles when reading in Feedly to allow safe for work reading",
	
	"version": "1.4",
	
	"permissions": [
		"http://feedly.com/*"
	],
	
	"content_scripts": [
		{
			"matches": ["http://feedly.com/*", "https://feedly.com/"],
			"js": ["jquery.js", "bleendly.js"],
			"css": ["seemply.css"]
		}
	]
}